{
  "compilerOptions": {
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "target": "esnext",
    "skipLibCheck": true,
    "rootDirs": [
      "./"
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@_tabs_/*": [
        "./_tabs_/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@api/*": [
        "./api/*"
      ],
      "@context/*": [
        "./context/*"
      ],
      "@assets/*": [
        "./assets/*"
      ]
    }
  },
  "extends": "expo/tsconfig.base"
}
